	Результат.Колонки.Добавить(ИмяКолонкиИД);
	ОбработатьТаблицуТоваров(Результат, ИмяКолонкиИД);
	
	ОбщегоНазначенияЭД.ЗагрузитьТаблицуВДерево(ДеревоДанных, Результат, "Товары");
	
